{
  "name": "fastify",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "etsc && tsc-alias",
    "dev": "nodemon",
    "start": "node ./dist/main.js"
  },
  "devDependencies": {
    "config": "*",
    "esbuild": "^0.14.2",
    "esbuild-node-tsc": "^1.8.2",
    "nodemon": "^2.0.15",
    "tsc-alias": "^1.4.2",
    "tsconfig": "*"
  },
  "dependencies": {
    "fastify": "^3.24.1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ignore": [
      "src/**/*.test.ts"
    ],
    "ext": "ts,mjs,js,json,graphql",
    "exec": "etsc && tsc-alias && node ./dist/main.js",
    "legacyWatch": true
  }
}
